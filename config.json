{
  "fields":
  {
    "surname": {
      "maskingType": "redact",
      "replacement": "*"
    },
    "email": {
      "maskingType": "regex",
      "pattern": "(?<=^.).*(?=.*@)",
      "replacement": "*******"
    },
    "phoneNumber": {
      "maskingType": "partial",
      "visiblePrefix": 0,
      "visibleSuffix": 4,
      "replacement": "*"
    },
    "dob": {
      "maskingType": "regex",
      "pattern": "[0-9]",
      "replacement": "#"
    }
  }
}