{
  "fields":
  {
    "surname": {
      "maskingType": "redact",
      "replacement": "*"
    },
    "email": {
      "maskingType": "regex",
      "pattern": "(?<=^.).*(?=.*@)",
      "replacement": "*******"
    },
    "phoneNumber": {
      "maskingType": "scrambleInt",
      "min": 0,
      "max": 9,
      "length": "None"
    },
    "dob": {
      "maskingType": "regex",
      "pattern": "[0-9]",
      "replacement": "#"
    }
  }
}